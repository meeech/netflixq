{
  "name": "netflixQ",
  "version": "1.0.6",
  "description": "Add queue functionality to netflix.ca for digital/instant movies.",
  "background_page": "background.html",
  "browser_action": {
    "default_icon": "star_red.png",
    "popup": "netflixq.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.netflix.ca/*","http://www.netflix.com/*"],
      "js": ["simpleyui-min.js", "pagescript.js"],
      "css": ["netflixq.css"]
    }
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}